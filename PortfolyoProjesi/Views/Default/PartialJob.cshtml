@model List<PortfolyoProjesi.Models.Tbl_Job>


        <div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
            <h3 class="resume-title">Deneyim</h3>

            @foreach (var item in Model)
            {
                <div class="resume-item">
                    <h4>@item.Title</h4>
                    <h5>@item.StartDate</h5>
                    <p><em>@item.CompanyName</em></p>
                    <ul>@item.Description</ul>
                </div>
            }

        </div>

